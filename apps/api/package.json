{
  "name": "@framecraft/api",
  "version": "0.1.0",
  "private": true,
  "description": "FrameCraft serverless API for Shopify Admin API integration",
  "type": "module",
  "main": "./src/index.ts",
  "scripts": {
    "build": "tsc --build",
    "dev": "tsc --build --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "clean": "rimraf dist"
  },
  "dependencies": {

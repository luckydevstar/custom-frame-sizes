    "cookie": "^0.6.0"
  },
  "devDependencies": {
    "@types/cookie": "^0.6.0",
    "@types/node": "^20.11.0",
    "@vercel/node": "^3.0.0",
    "typescript": "^5.6.3",
    "vitest": "^1.2.0"
  },
  "keywords": [
    "framecraft",
    "api",
    "serverless",
    "shopify",
    "vercel"
